    <title>Admin</title>
</head>
<body>
    <!-- As a link -->
    <nav class="navbar navbar-expand-lg navbar-white text-white bg-dark">
     
    
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item active ">
              <a class="nav-link text-white h3" href="/">Home </a>
            </li>
            <li class="nav-item" style="margin-left: 15px;">
              <a class="nav-link text-white h3" href="conect">Contact us</a>
            </li>
            
          </ul>
        </div>
      </nav>
      <div style=" width: 90%; margin: auto; margin-top: 25px;">
      <nav class="navbar navbar-expand-lg navbar-light bg-dark">
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
          <div class="navbar-nav">
            <a class="nav-link h5 text-white mr-3" href="Mumbai">Mumbai</a>
            <a class="nav-link h5 text-white mr-3" href="Jodhpur">Jodhpur</a>
